
@{
    ViewBag.Title = "Загрузка заданий";
}

<h2>Чтобы загрузить задания выберите файл и нажмите загрузить</h2>
@using (Html.BeginForm("UploadFile", "TaskLoad", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div class="form-group">
        <input type="file" accept="text/xml" name="xmlfile">
    </div>
    <input type="submit" value="Загрузить">
}

